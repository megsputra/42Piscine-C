#include <unistd.h>
//function prototypes
int ft_putchar(char c);
void ft_print_alphabet(void);

int main()
{
	ft_print_alphabet();
	return 0;
}

int ft_putchar(char c)
{
  write(1, 
void ft_print_alphabet(void)
{
	char l = 'a';
	while(l <= 'z')
	{
		ft_putchar(l);
		l+=1;
	}
}
